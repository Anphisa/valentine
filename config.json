{
  "name": "Clustering_run_1",
  "source": {
    "type": "InstanceLoader",
    "args": {
      "data": "data/framework_tests/Prospect_approx_columns_test_source.csv"
    }
  },
  "target": {
    "type": "InstanceLoader",
    "args": {
      "data": "data/framework_tests/Prospect_approx_columns_test_target.csv"
    }
  },
  "algorithm": {
    "type": "CorrelationClustering",
    "args": {
      "threshold1": 0.1,
      "threshold2": 0.1,
      "quantiles": 256,
      "process_num": 4,
      "chunk_size": 1,
      "clear_cache": true
    }
  },
  "metrics": {
    "names": ["precision", "recall", "precision_at_n_percent", "recall_at_sizeof_ground_truth"],
    "args": {
      "n": 50
    }
  },
  "golden_standard": "data/framework_tests/Prospect_approx_columns_test_mapping.json"
}